            marpaESLIF_wrapperShr.marpaESLIF_memcpy(marpaESLIFValueResultp, marpaESLIFValueResultFloatp, @SIZEOF_marpaESLIFValueResult_t@);
            Marshal.FreeHGlobal(marpaESLIFValueResultFloatp);

